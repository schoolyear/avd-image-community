{
  version: 'v2',
  name: 'com.schoolyear.win11-office365',
  description: "Configure the base image to win11-office365 and whitelists the required URLs. No default language support.",
  author: {
    name: 'Schoolyear',
    email: 'avd@schoolyear.com'
  },
  platform_version: "2",
  base_image: {
    type: "PlatformImage",
    publisher: "MicrosoftWindowsDesktop",
    version: "latest",
    sku: "win11-24h2-avd-m365",
    offer: "office-365"
  },
  network: {
    http_proxy_whitelist: [
      "officeclient.microsoft.com:443",
      "odc*.officeapps.live.com:443",
      "ols.officeapps.live.com:443",
      "licensing.m365.svc.cloud.microsoft:443"
    ],
  }
}